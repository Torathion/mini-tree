{
  "name": "mini-tree",
  "version": "1.0.0",
  "description": "A tiny universal tree data structure that just works!",
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "lint": "eslint src --fix",
    "format": "npx prettier --write */**.ts",
    "test": "vitest"
  },
  "keywords": [
    "tree",
    "data structure",
    "utility",
    "tiny",
    "small"
  ],
  "author": "Torathion",
  "homepage": "https://github.com/Torathion/mini-tree",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torathion/mini-tree.git"
  },
  "bugs": {
    "url": "https://github.com/Torathion/mini-tree/issues"
  },
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@vitest/coverage-v8": "^3.0.9",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "jiti": "^2.4.2",
    "eslint-config-shiny": "^4.1.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
